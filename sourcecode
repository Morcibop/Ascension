        infoChar.setVisible(true);
        infoTalents.setVisible(false);
    }                                  

    private void charListActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String selection = String.valueOf(charList.getSelectedItem());

        /*
        URL nc = GUI.class.getResource("/namecards/.png");
        ImageIcon = new ImageIcon(nc);
        bAscencion.setIcon();
        break;
        */

        switch(selection){
            case "Albedo": //Select Albedo Namecard
                URL ncAlbedo = GUI.class.getResource("/namecards/Albedo.png");
                ImageIcon Albedo = new ImageIcon(ncAlbedo);
                bAscencion.setIcon(Albedo);
                charSelected.setText("Albedo");
              break;
            case "Amber": //Select Amber Namecard
