    private void StAddRollMousePressed(java.awt.event.MouseEvent evt) {                                       
        URL uaf2 = GUI.class.getResource("/resources/Acquaint Fate 2.png");
        ImageIcon af2 = new ImageIcon(uaf2);
        StAddRoll.setIcon(af2);
        StAddRoll.setSize(60,60);
        StAddRoll.setLocation(260, 51);
    }                                      

    private void StAddRollMouseReleased(java.awt.event.MouseEvent evt) {                                        
        URL uaf = GUI.class.getResource("/resources/Acquaint Fate.png");
        ImageIcon af = new ImageIcon(uaf);
        StAddRoll.setIcon(af);
        StAddRoll.setSize(42,42);
        StAddRoll.setLocation(269, 61);
    }                                       

