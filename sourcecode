    private void StAddRollMouseClicked(java.awt.event.MouseEvent evt) {                                       
        StdMegaPity++;
        if (StdMegaPity == 90) {
            StdMegaPity = 0;
            StMegaPity.setText(""+StdMegaPity);
            rolls.put("Standard","Mega Pity", StdMegaPity);
        } else {
            StMegaPity.setText(""+StdMegaPity);
            rolls.put("Standard","Mega Pity", StdMegaPity);
        }
        
        StdMiniPity++;
        if (StdMiniPity == 10) {
            StdMiniPity = 0;
            StMiniPity.setText(""+StdMiniPity);
            rolls.put("Standard","Mini Pity", StdMiniPity);
        } else {
            StMiniPity.setText(""+StdMiniPity);
            rolls.put("Standard","Mini Pity", StdMiniPity);
        }
        
        StdTotal++;
        StTotal.setText(""+StdTotal);
        rolls.put("Standard","Total", StdTotal);
        
        try {
            rolls.store();
        } catch (IOException ex) {
            Logger.getLogger(GUI.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                      

    private void CharAddRollMouseClicked(java.awt.event.MouseEvent evt) {                                         
        ChMegaPity++;
        if (ChMegaPity == 90) {
            ChMegaPity = 0;
            CharMegaPity.setText(""+ChMegaPity);
            rolls.put("Character","Mega Pity", ChMegaPity);
        } else {
            CharMegaPity.setText(""+ChMegaPity);
            rolls.put("Character","Mega Pity", ChMegaPity);
        }
        
        ChMiniPity++;
        if (ChMiniPity == 10) {
            ChMiniPity = 0;
            CharMiniPity.setText(""+ChMiniPity);
            rolls.put("Character","Mini Pity", ChMiniPity);
        } else {
            CharMiniPity.setText(""+ChMiniPity);
            rolls.put("Character","Mini Pity", ChMiniPity);
        }
        
        ChTotal++;
        CharTotal.setText(""+ChTotal);
        rolls.put("Character","Total", ChTotal);
        
        try {
            rolls.store();
        } catch (IOException ex) {
            Logger.getLogger(GUI.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                        

    private void WeapAddRollMouseClicked(java.awt.event.MouseEvent evt) {                                         
        WpMegaPity++;
        if (WpMegaPity == 80) {
            WpMegaPity = 0;
            WeapMegaPity.setText(""+WpMegaPity);
            rolls.put("Weapon","Mega Pity", WpMegaPity);
        } else {
            WeapMegaPity.setText(""+WpMegaPity);
            rolls.put("Weapon","Mega Pity", WpMegaPity);
        }
        
        WpMiniPity++;
        if (WpMiniPity == 10) {
            WpMiniPity = 0;
            WeapMiniPity.setText(""+WpMiniPity);
            rolls.put("Weapon","Mini Pity", WpMiniPity);
        } else {
            WeapMiniPity.setText(""+WpMiniPity);
            rolls.put("Weapon","Mini Pity", WpMiniPity);
        }
        
        WpTotal++;
        WeapTotal.setText(""+WpTotal);
        rolls.put("Weapon","Total", WpTotal);
        
        try {
            rolls.store();
        } catch (IOException ex) {
            Logger.getLogger(GUI.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                        

    private void StResetMouseClicked(java.awt.event.MouseEvent evt) {                                     
        StdMegaPity = 0;
        StdMiniPity = 0;
        StdTotal = 0;
        
        rolls.put("Standard","Mega Pity", StdMegaPity);
        rolls.put("Standard","Mini Pity", StdMiniPity);
        rolls.put("Standard","Total", StdTotal);
        
        StMegaPity.setText("0");
        StMiniPity.setText("0");
        StTotal.setText("0");
        
        try {
            rolls.store();
        } catch (IOException ex) {
            Logger.getLogger(GUI.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                    

    private void StSetMouseClicked(java.awt.event.MouseEvent evt) {                                   
        JTextField StMeP = new JTextField(3);
        JTextField StMiP = new JTextField(3);
        JTextField StT = new JTextField(3);
        
        JPanel setST = new JPanel();
        
        setST.add(new JLabel("Mega Pity: "));
        setST.add(StMeP);
        setST.add(Box.createHorizontalStrut(5));
        setST.add(new JLabel("Mini Pity: "));
        setST.add(StMiP);
        setST.add(Box.createHorizontalStrut(5));
        setST.add(new JLabel("Total: "));
        setST.add(StT);
        
        int result = JOptionPane.showConfirmDialog(null, setST, 
               "STANDARD BANNER ROLLS", JOptionPane.OK_CANCEL_OPTION);
        if (result == JOptionPane.OK_OPTION) {
            StdMegaPity = Integer.parseInt(StMeP.getText());
            StdMiniPity = Integer.parseInt(StMiP.getText());
            StdTotal = Integer.parseInt(StT.getText());

            rolls.put("Standard","Mega Pity", StdMegaPity);
            rolls.put("Standard","Mini Pity", StdMiniPity);
            rolls.put("Standard","Total", StdTotal);

            StMegaPity.setText(""+StdMegaPity);
            StMiniPity.setText(""+StdMiniPity);
            StTotal.setText(""+StdTotal);
      }
    }                                  

    private void CharSetMouseClicked(java.awt.event.MouseEvent evt) {                                     
        JTextField CharMeP = new JTextField(3);
        JTextField CharMiP = new JTextField(3);
        JTextField CharT = new JTextField(3);
        
        JPanel setST = new JPanel();
        
        setST.add(new JLabel("Mega Pity: "));
        setST.add(CharMeP);
        setST.add(Box.createHorizontalStrut(5));
        setST.add(new JLabel("Mini Pity: "));
        setST.add(CharMiP);
        setST.add(Box.createHorizontalStrut(5));
        setST.add(new JLabel("Total: "));
        setST.add(CharT);
        
        int result = JOptionPane.showConfirmDialog(null, setST, 
               "CHARACTER BANNER ROLLS", JOptionPane.OK_CANCEL_OPTION);
        if (result == JOptionPane.OK_OPTION) {
            ChMegaPity = Integer.parseInt(CharMeP.getText());
            ChMiniPity = Integer.parseInt(CharMiP.getText());
            ChTotal = Integer.parseInt(CharT.getText());

            rolls.put("Character","Mega Pity", ChMegaPity);
            rolls.put("Character","Mini Pity", ChMiniPity);
            rolls.put("Character","Total", ChTotal);

            CharMegaPity.setText(""+ChMegaPity);
            CharMiniPity.setText(""+ChMiniPity);
            CharTotal.setText(""+ChTotal);
        }
    }                                    

