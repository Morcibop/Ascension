    private void AscencionMouseClicked(java.awt.event.MouseEvent evt) {                                       
        pAscencion.setVisible(true);
        bWeapons.setVisible(false);
        pInfo.setVisible(false);
        pRolls.setVisible(false);
        
        lDogeText.setVisible(false);
        
        Ascencion.setBackground(java.awt.Color.lightGray);
        Weapons.setBackground(java.awt.Color.white);
        Info.setBackground(java.awt.Color.white);
        Rolls.setBackground(java.awt.Color.white);
    }                                      

