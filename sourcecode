    private void bSplashMouseClicked(java.awt.event.MouseEvent evt) {                                     
        bSplash.setVisible(false);
        Menu.setVisible(true);
        Tabs.setVisible(true);
        
        pAscencion.setVisible(true);
        bWeapons.setVisible(false);
        pInfo.setVisible(false);
            infoTalents.setVisible(false);
        pRolls.setVisible(false);
        
        Ascencion.setBackground(java.awt.Color.lightGray);
    }                                    

    private void bListMouseClicked(java.awt.event.MouseEvent evt) {                                   

        if (count == 0){
            charList.showPopup();
            count++;
        } else if (count == 1){
            charList.hidePopup();
            count--;
        }
    }                                  

    private void formKeyPressed(java.awt.event.KeyEvent evt) {                                
        if(evt.getKeyCode() == KeyEvent.VK_F5 && AoT == false){
            setAlwaysOnTop(true);
            AoT = true;
        } else {
            setAlwaysOnTop(false);
            AoT = false;
        } 
    }                               

    private void bListMousePressed(java.awt.event.MouseEvent evt) {                                   
        bList.setSize(13,9);
        bList.setLocation(161, 228);
    }                                  

    private void bListMouseReleased(java.awt.event.MouseEvent evt) {                                    
        bList.setSize(14,10);
        bList.setLocation(160, 227);
    }                                   

    private void sPhaseStateChanged(javax.swing.event.ChangeEvent evt) {                                    
        int value = sPhase.getValue();
       
        switch (value) {
            case 0:
                //Phase 1
                lSliver.setText("1");
                lFragment.setText("0");
                lChunk.setText("0");
                lGemstone.setText("0");
                lBoss.setText("0");
                lTalent1.setText("9");
                lTalent2.setText("0");
                lTalent3.setText("0");
                lTalent4.setText("0");
                lTalent5.setText("0");
                lCommon1.setText("21");
                lCommon2.setText("0");
                lCommon3.setText("0");
                lSpecialty.setText("3");
                break;
            case 1:
                //Phase 2
                lSliver.setText("1");
                lFragment.setText("3");
                lChunk.setText("0");
                lGemstone.setText("0");
                lBoss.setText("2");
                lTalent1.setText("9");
                lTalent2.setText("0");
                lTalent3.setText("0");
                lTalent4.setText("0");
                lTalent5.setText("0");
                lCommon1.setText("36");
                lCommon2.setText("0");
                lCommon3.setText("0");
                lSpecialty.setText("13");
                break;
            case 2:
                //Phase 3
                lSliver.setText("1");
                lFragment.setText("9");
                lChunk.setText("0");
                lGemstone.setText("0");
                lBoss.setText("6");
                lTalent1.setText("9");
                lTalent2.setText("18");
                lTalent3.setText("0");
                lTalent4.setText("0");
                lTalent5.setText("0");
                lCommon1.setText("36");
                lCommon2.setText("33");
                lCommon3.setText("0");
                lSpecialty.setText("33");
                break;
            case 3:
                //Phase 4
                lSliver.setText("1");
                lFragment.setText("9");
                lChunk.setText("3");
                lGemstone.setText("0");
                lBoss.setText("14");
                lTalent1.setText("9");
                lTalent2.setText("63");
                lTalent3.setText("0");
                lTalent4.setText("0");
                lTalent5.setText("0");
                lCommon1.setText("36");
                lCommon2.setText("96");
                lCommon3.setText("0");
                lSpecialty.setText("63");
                break;
            case 4:
                //Phase 5
                lSliver.setText("1");
                lFragment.setText("9");
                lChunk.setText("9");
                lGemstone.setText("0");
                lBoss.setText("26");
                lTalent1.setText("9");
                lTalent2.setText("63");
                lTalent3.setText("30");
                lTalent4.setText("6");
                lTalent5.setText("0");
                lCommon1.setText("36");
                lCommon2.setText("96");
                lCommon3.setText("42");
                lSpecialty.setText("108");
                break;
            case 5:
                //Phase 6
                lSliver.setText("1");
                lFragment.setText("9");
                lChunk.setText("9");
                lGemstone.setText("6");
                lBoss.setText("46");
                lTalent1.setText("9");
                lTalent2.setText("63");
                lTalent3.setText("114");
                lTalent4.setText("18");
                lTalent5.setText("3");
                lCommon1.setText("36");
                lCommon2.setText("96");
                lCommon3.setText("129");
                lSpecialty.setText("168");
                break;
            default:
                break;
        }
        
    }                                   

    private void cAlbedoMouseEntered(java.awt.event.MouseEvent evt) {                                     
        URL cAlbedoBig = GUI.class.getResource("/cards/Albedo Big.png");
        ImageIcon cardAlbedo = new ImageIcon(cAlbedoBig);
        cAlbedo.setIcon(cardAlbedo);
        cAlbedo.setSize(147,175);
        cAlbedo.setLocation(2,2);
    }                                    

    private void cAlbedoMouseExited(java.awt.event.MouseEvent evt) {                                    
        URL cAlbedoSmall = GUI.class.getResource("/cards/Albedo.png");
        ImageIcon cardAlbedo = new ImageIcon(cAlbedoSmall);
        cAlbedo.setIcon(cardAlbedo);
        cAlbedo.setSize(132,158);
        cAlbedo.setLocation(10,10);
    }                                   

