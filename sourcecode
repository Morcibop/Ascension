    private void bDogeMouseClicked(java.awt.event.MouseEvent evt) {                                   
        lDogeText.setVisible(true);
        
        try {
            URL defaultSound = getClass().getResource("/resources/Bop.wav");
            AudioInputStream audioInputStream = AudioSystem.getAudioInputStream(defaultSound);
            Clip clip = AudioSystem.getClip();
            clip.open(audioInputStream);
            clip.start( );
        } catch (Exception ex) {
            ex.printStackTrace();
        }
    }                                  

    private void bDogeMousePressed(java.awt.event.MouseEvent evt) {                                   
        URL buttonSmall = GUI.class.getResource("/resources/Button Small.png");
        ImageIcon buttonS = new ImageIcon(buttonSmall);
        bDoge.setIcon(buttonS);
        bDoge.setSize(155,23);
        bDoge.setLocation(385,192);
        
        lArrow.setLocation(380, 200);
    }                                  

    private void bDogeMouseReleased(java.awt.event.MouseEvent evt) {                                    
        URL buttonLarge = GUI.class.getResource("/resources/Button.png");
        ImageIcon buttonL = new ImageIcon(buttonLarge);
        bDoge.setIcon(buttonL);
        bDoge.setSize(169,25);
        bDoge.setLocation(380,190);
        
        lArrow.setLocation(375, 198);
    }                                   

    private void bDogeMouseEntered(java.awt.event.MouseEvent evt) {                                   
        lArrow.setVisible(true);
    }                                  

    private void bDogeMouseExited(java.awt.event.MouseEvent evt) {                                  
        lArrow.setVisible(false);
    }                                 

