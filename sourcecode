        try{
            // load a custom font in your project folder
                InputStream unbt = GUI.class.getResourceAsStream("/resources/big_noodle_titling.ttf");
                nbt = Font.createFont(Font.TRUETYPE_FONT, unbt).deriveFont(16.5f);	
                GraphicsEnvironment ge = GraphicsEnvironment.getLocalGraphicsEnvironment();
                ge.registerFont(Font.createFont(Font.TRUETYPE_FONT, unbt));		
            }
            catch(IOException | FontFormatException e){
            }
        
        lAscencion.setFont(nbt);
        lWeapons.setFont(nbt);
